{
  "version": 2,
  "routes": [
    { "src": "/badge(?:/|$)", "dest": "/api/server.js" },
    { "src": "/badge/.*", "dest": "/api/server.js" },
    { "src": "/image(?:/|$)", "dest": "/api/server.js" },
    { "src": "/image/.*", "dest": "/api/server.js" },
    { "src": "/api/.*", "dest": "/api/server.js" },
    { "src": "/auth/.*", "dest": "/api/server.js" },
    { "src": "/(.*)", "dest": "/public/$1" }
  ]
}

